    def add(stri):
        obj = datetime.strptime(stri, "%b")
        month_number = obj.month
        return month_number

    def get_symbol(tic):
        li = list(filter(None, re.split(r'(\d+)', tic)))
        return li[0]

    exp_file_path = r"C:\Users\admin\Downloads\MonthlyExpiry.csv"
